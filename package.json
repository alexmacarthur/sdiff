{
  "name": "striff",
  "description": "Real simple string diffing.",
  "author": "Alex MacArthur (https://macarthur.me)",
  "version": "1.0.5",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "string diffing",
    "difference",
    "differ"
  ],
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "start": "vite",
    "test": "vitest run src",
    "build": "tsc --noEmit --emitDeclarationOnly false && vite build && tsc",
    "format": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "prettier": "^2.8.1",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  }
}
